{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "author": "Glen Holcomb <glen@leakybucket.dev>",
  "name": "Less is More",
  "themes": [
    {
      "appearance": "dark",
      "name": "Less is More",
      "style": {
        "background": "#0A0A0A",
        "background.appearance": "opaque",
        "border": "#C063F240",
        "border.disabled": "#C063F220",
        "border.focused": "#C063F260",
        "border.selected": "#C063F260",
        "editor.active_line.background": "#E58B1430",
        "editor.background": "#0A0A0A",
        "editor.gutter.background": "#0A0A0A",
        "editor.highlighted_line.background": "#FFD09750",
        "editor.subheader.background": "#0A0A0A",
        "element.background": "#0A0A0A",
        "ghost_element.background": "#B992CE50",
        "status_bar.background": "#89898930",
        "tab.active_background": "#E58B1430",
        "scrollbar.thumb.background": "#E0B6F940",
        "scrollbar.track.background": "#0A0A0A",
        "scrollbar.track.border": "#C063F240"
      }
    }
  ]
